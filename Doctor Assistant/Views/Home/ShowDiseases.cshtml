@{
    ViewData["Title"] = "Show Diseases";
}


<link rel="stylesheet" href="~/css/ShowDiseases.css" />
<link href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" />
<link href="https://stackpath.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.bundle.min.js" />
<link href="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js" />

<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>


<div class="page-content page-container" id="page-content">
    <div class="padding">
        <div class="row container d-flex justify-content-center">

            <div class="col-lg-12 col-xl-6">
                <div class="card">
                    <div class="card-block">
                        <div class="row" id="sortable">

                            <div class="col-md-6 m-b-20" draggable="false" style="">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">
                                    <div class="card-sub">
                                        <img class="card-img-top img-fluid" src="https://www.pantechelearning.com/wp-content/uploads/2020/12/Brain-Tumour-segmentation-using-Convolutional-Neural-Network-Matlab.jpg" alt="Card image cap">
                                        
                                        <div class="card-block">
                                            <h4 class="card-title">Brain Tumor Classification</h4>
                                            <p class="card-text">
                                                Brain Tumor Classification by MRI image using
                                                Convolutional Neural Networks (CNN)
                                            </p>
                                        </div>
                                    </div>
                                </a>
                            </div>

                            <div class="col-md-6">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">
                                    <div class="card-sub m-b-20">
                                        <img class="card-img-top img-fluid" src="http://nurseslabs.com/wp-content/uploads/2016/10/FB-Cerebrovascular-Accident-Stroke-Nursing-Care-Management.png" alt="Card image cap">
                                        <div class="card-block">
                                            <h4 class="card-title">Stroke Disease Detection</h4>
                                            <p class="card-text">
                                                Stroke Disease Detection and Prediction Using Robust
                                                Learning Approach
                                            </p>
                                        </div>
                                    </div>
                                </a>
                            </div>

                            <div class="col-md-6" style="">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">
                                    <div class="card-sub">
                                        <img class="card-img-top img-fluid" src="https://scx2.b-cdn.net/gfx/news/hires/2022/alzheimers-4.jpg" alt="Card image cap" draggable="false">
                                        <div class="card-block">
                                            <h4 class="card-title">Alzheimer's Disease Detection</h4>
                                            <p class="card-text">
                                                Alzheimer's disease detection from structural MRI
                                                using conditional deep triplet network
                                            </p>
                                        </div>
                                    </div>
                                </a>
                            </div>

                            <div class="col-md-6 m-b-20" draggable="false" style="">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">
                                    <div class="card-sub">
                                        <img class="card-img-top img-fluid" src="https://image.slidesharecdn.com/dreyescanciti-190716233007/95/diabetic-retinopathy-detection-using-cnn-image-classification-algorithm-1-638.jpg?cb=1563319977" alt="Card image cap">
                                        <div class="card-block">
                                            <h4 class="card-title">Diabetic Retinopathy Detection</h4>
                                            <p class="card-text">
                                                Automatic Detection of Diabetic Retinopathy in Retinal
                                                Fundus Photographs Based on Deep Learning
                                                Algorithm.
                                            </p>
                                        </div>
                                    </div>
                                </a>
                            </div>
                           
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<script>

    $(document).ready(function () {
        $("#sortable").sortable();
        $("#sortable").disableSelection();
    });

</script>
